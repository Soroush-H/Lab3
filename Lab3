for $a in /mondial/organization
where $a/contains(data(name),"Nuclear") and empty($a/established)


return $a/name

-- n√§rmast hittils
for $a in /mondial/organization
for $b in /mondial/country
where $a/contains(data(name),"Nuclear")
and empty($a/established)
and $b[@car_code = $a/@country]/encompassed = "Europe"


return $a/name

